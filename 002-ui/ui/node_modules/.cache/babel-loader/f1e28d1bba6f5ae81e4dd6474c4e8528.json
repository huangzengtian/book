</td>\r\n          <td>作者</td>\r\n          <td>出版日期</td>\r\n          <td>价格</td>\r\n          <td>描述</td>\r\n          <td>数量</td>\r\n          <td>操作</td>\r\n        </tr>\r\n      </thead>\r\n      <tbody v-for=\"(item, index) in books\">\r\n        <tr>\r\n          <td>{{index}}</td>\r\n          <td>{{item.name | nameType}}</td>\r\n          <td>{{item.author}}</td>\r\n          <td>{{item.publishDate}}</td>\r\n          <td>{{item.price}}</td>\r\n          <td>{{item.description}}</td>\r\n          <td>{{item.num}}</td>\r\n          <td><button>删除</button></td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from \"axios\";\r\n  import api from \"@/config/api\";\r\n\r\n  export default {\r\n    name: \"Book\",\r\n    data() {\r\n      return {\r\n        books: []\r\n      }\r\n    },\r\n    filters: {\r\n      nameType(name) {\r\n        return \"《\" + name + \"》\";\r\n      }\r\n    },\r\n    created() {\r\n      let that = this;\r\n      axios.post(api.GET_ALL_BOOK).then(r => {\r\n        that.books = r.data.data.tbBookList;\r\n        let array = [];\r\n        that.books.map((item,index)=>{\r\n          array.push(\r\n              Object.assign({}, item, {'num': 1})\r\n          )\r\n        });\r\n\r\n      })\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components"}]}