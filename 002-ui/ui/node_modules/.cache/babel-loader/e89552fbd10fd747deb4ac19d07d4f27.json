AAA,IAAA,CAAA,CAAA,MAAA,CAAA,aAAA,CAAA;AACA,KANA;AAOA,IAAA,SAPA,qBAOA,KAPA,EAOA;AACA,aAAA,MAAA,MAAA,CAAA,KAAA,CAAA,CAAA,OAAA,CAAA,CAAA,CAAA;AACA;AATA,GAnBA;AA8BA,EAAA,OA9BA,qBA8BA;AACA,QAAA,IAAA,GAAA,IAAA;AACA,IAAA,KAAA,CAAA,IAAA,CAAA,GAAA,CAAA,YAAA,EAAA,IAAA,CAAA,UAAA,CAAA,EAAA;AACA,MAAA,IAAA,CAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,IAAA,CAAA,UAAA;AACA,KAFA;AAGA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <label for=\"searchName\">\r\n      名称<input type=\"text\" id=\"searchName\" v-model=\"searchName\">&nbsp;\r\n    </label>\r\n    <label for=\"searchAuthor\">\r\n      作者<input type=\"text\" id=\"searchAuthor\" v-model=\"searchAuthor\">&nbsp;\r\n    </label>\r\n    <label>\r\n      价格<input type=\"number\" id=\"searchPriceLow\" v-model=\"searchPriceLow\">\r\n      - <input type=\"number\" id=\"searchPriceHigh\" v-model=\"searchPriceHigh\">&nbsp;\r\n    </label>\r\n    <button @click=\"searchBook\">搜索</button>\r\n    <br>\r\n    <br>\r\n    <table cellpadding=\"0\" cellspacing=\"0\" border=\"1px\">\r\n      <thead>\r\n      <tr>\r\n        <td>序号</td>\r\n        <td>书名</td>\r\n        <td>作者</td>\r\n        <td>出版日期</td>\r\n        <td>价格</td>\r\n        <td>描述</td>\r\n      </tr>\r\n      </thead>\r\n      <tbody v-for=\"(item, index) in books\">\r\n      <tr>\r\n        <td>{{index+1}}</td>\r\n        <td>{{item.name | nameType}}</td>\r\n        <td>{{item.author}}</td>\r\n        <td>{{item.publishDate | dateType}}</td>\r\n        <td>{{item.price | priceType}}</td>\r\n        <td>{{item.description}}</td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from \"axios\";\r\n  import api from \"@/config/api\";\r\n  import moment from \"moment\";\r\n\r\n  export default {\r\n    name: \"Index\",\r\n    data() {\r\n      return {\r\n        books: [],\r\n        searchName: \"\",\r\n        searchAuthor: \"\",\r\n        searchPriceLow: \"\",\r\n        searchPriceHigh: \"\"\r\n      }\r\n    },\r\n    methods: {\r\n      searchBook() {\r\n        let that = this;\r\n        axios.post(api.GET_ALL_BOOK_BY_FUZZY).then(r => {\r\n          that.books = r.data.tbListBook;\r\n        })\r\n      }\r\n    },\r\n    filters: {\r\n      nameType(name) {\r\n        return \"《\" + name + \"》\";\r\n      },\r\n      dateType(date) {\r\n        return  moment(date).format('YYYY年MM月DD日');\r\n      },\r\n      priceType(price) {\r\n        return \"￥\" + Number(price).toFixed(2);\r\n      }\r\n    },\r\n    created() {\r\n      let that = this;\r\n      axios.post(api.GET_ALL_BOOK).then(r => {\r\n        that.books = r.data.data.tbBookList;\r\n      })\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components"}]}