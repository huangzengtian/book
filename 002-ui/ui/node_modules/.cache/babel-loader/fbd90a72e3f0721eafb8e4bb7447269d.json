AOA,MAAA,KAAA,CAAA,IAAA,CAAA,GAAA,CAAA,2BAAA,EAAA,KAAA,EAAA,IAAA,CAAA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA;AAxBA,GAXA;AAqCA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,oBACA,IADA,EACA;AACA,aAAA,MAAA,IAAA,GAAA,GAAA;AACA,KAHA;AAIA,IAAA,QAJA,oBAIA,IAJA,EAIA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,CAAA,MAAA,CAAA,aAAA,CAAA;AACA,KANA;AAOA,IAAA,SAPA,qBAOA,KAPA,EAOA;AACA,aAAA,MAAA,MAAA,CAAA,KAAA,CAAA,CAAA,OAAA,CAAA,CAAA,CAAA;AACA;AATA,GArCA;AAgDA,EAAA,OAhDA,qBAgDA;AAAA;;AACA,QAAA,KAAA,GAAA;AACA,MAAA,IAAA,EAAA,EADA;AAEA,MAAA,MAAA,EAAA;AAFA,KAAA;AAKA,IAAA,KAAA,CAAA,IAAA,CACA,GAAA,CAAA,qBADA,EAEA,KAFA,EAGA,IAHA,CAGA,UAAA,CAAA,EAAA;AACA,MAAA,MAAA,CAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,IAAA;AACA,KALA;AAMA;AA5DA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <label for=\"searchName\">\r\n      名称<input type=\"text\" id=\"searchName\" v-model.trim=\"searchName\">&nbsp;\r\n    </label>\r\n    <label for=\"searchAuthor\">\r\n      作者<input type=\"text\" id=\"searchAuthor\" v-model.trim=\"searchAuthor\">&nbsp;\r\n    </label>\r\n    <label>\r\n      价格<input type=\"number\" id=\"searchPriceLow\" v-model.trim=\"searchPriceLow\">\r\n      - <input type=\"number\" id=\"searchPriceHigh\" v-model.trim=\"searchPriceHigh\">&nbsp;\r\n    </label>\r\n    <button @click=\"searchBook\">搜索</button>\r\n    <button @click=\"searchBookWithPrice\">包含价格区间的搜索</button>\r\n    <br>\r\n    <br>\r\n    <table cellpadding=\"0\" cellspacing=\"0\" border=\"1px\">\r\n      <thead>\r\n      <tr>\r\n        <td>序号</td>\r\n        <td>书名</td>\r\n        <td>作者</td>\r\n        <td>出版日期</td>\r\n        <td>价格</td>\r\n        <td>描述</td>\r\n      </tr>\r\n      </thead>\r\n      <tbody v-for=\"(item, index) in books\">\r\n      <tr>\r\n        <td>{{ index + 1 }}</td>\r\n        <td>{{ item.name | nameType }}</td>\r\n        <td>{{ item.author }}</td>\r\n        <td>{{ item.publishDate | dateType }}</td>\r\n        <td>{{ item.price | priceType }}</td>\r\n        <td>{{ item.description }}</td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from \"axios\";\r\n  import api from \"@/config/api\";\r\n  import moment from \"moment\";\r\n\r\n  export default {\r\n    name: \"Index\",\r\n    data() {\r\n      return {\r\n        books: [],\r\n        searchName: \"\",\r\n        searchAuthor: \"\",\r\n        searchPriceLow: \"\",\r\n        searchPriceHigh: \"\"\r\n      }\r\n    },\r\n    methods: {\r\n      searchBook() {\r\n        let param = {\r\n          name: this.searchName,\r\n          author: this.searchAuthor,\r\n          priceLow: this.searchPriceLow,\r\n          priceHigh: this.searchPriceHigh\r\n        };\r\n\r\n        axios.post(api.GET_ALL_BOOK_BY_FUZZY, param).then(r => {\r\n          this.books = r.data.data;\r\n        })\r\n      },\r\n      searchBookWithPrice() {\r\n        let param = {\r\n          name: this.searchName,\r\n          author: this.searchAuthor,\r\n          priceLow: this.searchPriceLow === '' ? 0 : this.searchPriceLow,\r\n          priceHigh: this.searchPriceHigh\r\n        };\r\n\r\n        axios.post(api.GET_BOOK_BY_FUZZY_AND_PRICE, param).then(r => {\r\n          this.books = r.data.data;\r\n        })\r\n      }\r\n    },\r\n    filters: {\r\n      nameType(name) {\r\n        return \"《\" + name + \"》\";\r\n      },\r\n      dateType(date) {\r\n        return moment(date).format('YYYY年MM月DD日');\r\n      },\r\n      priceType(price) {\r\n        return \"￥\" + Number(price).toFixed(2);\r\n      }\r\n    },\r\n    created() {\r\n      let param = {\r\n        name: '',\r\n        author: ''\r\n      }\r\n\r\n      axios.post(\r\n          api.GET_ALL_BOOK_BY_FUZZY,\r\n          param\r\n      ).then(r => {\r\n        this.books = r.data.data;\r\n      })\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components"}]}