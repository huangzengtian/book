{"remainingRequest":"F:\\ideaProject\\book-project\\002-ui\\ui\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!F:\\ideaProject\\book-project\\002-ui\\ui\\src\\components\\Index.vue?vue&type=script&lang=js&","dependencies":[{"path":"F:\\ideaProject\\book-project\\002-ui\\ui\\src\\components\\Index.vue","mtime":1622199594226},{"path":"F:\\ideaProject\\book-project\\002-ui\\ui\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\ideaProject\\book-project\\002-ui\\ui\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"F:\\ideaProject\\book-project\\002-ui\\ui\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\ideaProject\\book-project\\002-ui\\ui\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0CA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Index.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <div>\r\n    <label for=\"searchName\">\r\n      名称<input type=\"text\" id=\"searchName\" v-model.trim=\"searchName\">&nbsp;\r\n    </label>\r\n    <label for=\"searchAuthor\">\r\n      作者<input type=\"text\" id=\"searchAuthor\" v-model.trim=\"searchAuthor\">&nbsp;\r\n    </label>\r\n    <label>\r\n      价格<input type=\"number\" id=\"searchPriceLow\" v-model.trim=\"searchPriceLow\">\r\n      - <input type=\"number\" id=\"searchPriceHigh\" v-model.trim=\"searchPriceHigh\">&nbsp;\r\n    </label>\r\n    <button @click=\"searchBook\">搜索</button>\r\n    <button @click=\"searchBookWithPrice\">包含价格区间的搜索</button>\r\n    <br>\r\n    <br>\r\n    <table cellpadding=\"0\" cellspacing=\"0\" border=\"1px\">\r\n      <thead>\r\n      <tr>\r\n        <td>序号</td>\r\n        <td>书名</td>\r\n        <td>作者</td>\r\n        <td>出版日期</td>\r\n        <td>价格</td>\r\n        <td>描述</td>\r\n      </tr>\r\n      </thead>\r\n      <tbody v-for=\"(item, index) in books\">\r\n      <tr>\r\n        <td>{{ index + 1 }}</td>\r\n        <td>{{ item.name | nameType }}</td>\r\n        <td>{{ item.author }}</td>\r\n        <td>{{ item.publishDate | dateType }}</td>\r\n        <td>{{ item.price | priceType }}</td>\r\n        <td>{{ item.description }}</td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from \"axios\";\r\n  import api from \"@/config/api\";\r\n  import moment from \"moment\";\r\n\r\n  export default {\r\n    name: \"Index\",\r\n    data() {\r\n      return {\r\n        books: [],\r\n        searchName: \"\",\r\n        searchAuthor: \"\",\r\n        searchPriceLow: \"\",\r\n        searchPriceHigh: \"\"\r\n      }\r\n    },\r\n    methods: {\r\n      searchBook() {\r\n        let param = {\r\n          name: this.searchName,\r\n          author: this.searchAuthor,\r\n          priceLow: this.searchPriceLow,\r\n          priceHigh: this.searchPriceHigh\r\n        };\r\n\r\n        axios.post(api.GET_ALL_BOOK_BY_FUZZY, param).then(r => {\r\n          this.books = r.data.data;\r\n        })\r\n      },\r\n      searchBookWithPrice() {\r\n        let param = {\r\n          name: this.searchName,\r\n          author: this.searchAuthor,\r\n          priceLow: this.searchPriceLow === '' ? 0 : this.searchPriceLow,\r\n          priceHigh: this.searchPriceHigh\r\n        };\r\n\r\n        axios.post(api.GET_BOOK_BY_FUZZY_AND_PRICE, param).then(r => {\r\n          this.books = r.data.data;\r\n        })\r\n      }\r\n    },\r\n    filters: {\r\n      nameType(name) {\r\n        return \"《\" + name + \"》\";\r\n      },\r\n      dateType(date) {\r\n        return moment(date).format('YYYY年MM月DD日');\r\n      },\r\n      priceType(price) {\r\n        return \"￥\" + Number(price).toFixed(2);\r\n      }\r\n    },\r\n    created() {\r\n      let param = {\r\n        name: '',\r\n        author: ''\r\n      }\r\n\r\n      axios.post(\r\n          api.GET_ALL_BOOK_BY_FUZZY,\r\n          param\r\n      ).then(r => {\r\n        this.books = r.data.data;\r\n      })\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}